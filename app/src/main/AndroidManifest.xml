<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dada.mobile_thaidass">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/logo_aapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_aapp_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.detail_book.DetailBookActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.select_book.SelectBookActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.detail_dasspage.DetailDassPageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.present_Image.PresentImageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.main.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.splash.SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>